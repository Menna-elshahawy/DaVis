                        document.getElementById("tail").style.display='inline';
                        document.getElementById("upT").style.display='inline';
                        document.getElementById("tail").style.top='300';
                        document.getElementById("tail").style.left=410+(80*(list.length-1));
                        document.getElementById("upT").style.left=415+(80*(list.length-1));
                    }
                    
                  
                }

