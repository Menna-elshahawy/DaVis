                                                    document.getElementById("upH").style.display='inline';
                                                    document.getElementById("upL").style.display='none'; 
                                                    this.display();
                                                    document.getElementById("ih5").style.background='none';
                                                    document.getElementById("action").innerHTML="You inserted "+v+" at the head";
                                                    },5500);
                             
                                setTimeout(function(){document.getElementById("insHM").style.display='none';
                                                    document.getElementById("method").style.display='none';},8000);
                    }  
                     document.getElementById("inH").value="";
                    
                }
                
                function insertTail(){
                    document.getElementById("insH").style.display='none';
                    document.getElementById("insT").style.display='none';
                    document.getElementById("insI").style.display='none';   
                    document.getElementById("showT").style.display='none';
                    
                    var v = document.getElementById("inT").value;
                    
                    if(v==""){
                        document.getElementById("notify").innerHTML="You must enter a value";
                    }
                    
                    else{
                    document.getElementById("notify").innerHTML="";
                        
                        if(list.length==0){
                            list.push(v);
                            document.getElementById("action").innerHTML="You inserted "+v+" at the tail";
                            display();
                        }
                        
                        else if(list.length==7){
                            document.getElementById("notify").innerHTML="Maximum length is 7";
                        }
                        else{
                            if(exist(v)){
                                document.getElementById("notify").innerHTML="No duplicate values allowed";
                            }
                            else{


                                document.getElementById("insTM").style.display='inline';
                                document.getElementById("method").style.display='inline';
                                
                                const nodeT=document.getElementById("nodeT");
                                                               
                                document.getElementById("nodeT").innerHTML=v;
                                document.getElementById("rightT").style.left=448+(list.length-1)*80;
                                document.getElementById("upR").style.left=450+(list.length-1)*80;
                                var t='l'.concat(list.length);
                                nodeT.style.left=410+(list.length)*80;
                                
                                list.push(v);   

                                setTimeout(function(){document.getElementById("it1").style.background='#fbe8a6';
                                                    },1000);
                                
                                setTimeout(function(){document.getElementById("it1").style.background='none';
                                                    document.getElementById("it2").style.background='#fbe8a6';
                                                    nodeT.style.display='inline-block';
                                                    },2000);
                                
                                setTimeout(function(){document.getElementById("it3").style.background='#fbe8a6';
                                                    document.getElementById("it2").style.background='none';
                                                    if(list.length>1){
                                                        document.getElementById("rightT").style.display='inline';
                                                    }
                                                    },3000);
                                
                                setTimeout(function(){document.getElementById("it4").style.background='#fbe8a6';
                                                    document.getElementById("it3").style.background='none';
                                                    document.getElementById("upT").style.display='none';
                                                    document.getElementById("upR").style.display='inline';
                                                                                    
                                                    },4000);

                                setTimeout(function(){document.getElementById("it4").style.background='none';
                                                    document.getElementById("it5").style.background='#fbe8a6';
                                                    },5000);

                                setTimeout(function(){nodeT.style.display='none';
                                                    document.getElementById("rightT").style.display='none';
                                                    document.getElementById("upT").style.display='inline';
                                                    document.getElementById("upR").style.display='none'; 
                                                    this.display();
                                                    document.getElementById("it5").style.background='none';
                                                    document.getElementById("action").innerHTML="You inserted "+v+" at the tail";
                                                    },5500);
                             
                                setTimeout(function(){document.getElementById("insTM").style.display='none';
                                                    document.getElementById("method").style.display='none';},8000);



                                document.getElementById("notify").innerHTML="";

                            }    
                        }
                        document.getElementById("inT").value="";  
                    }    
                }

                function insertIndex(){
                    document.getElementById("insH").style.display='none';
                    document.getElementById("insT").style.display='none';
                    document.getElementById("insI").style.display='none';  
                    
                    
                    var v=document.getElementById("inI").value;
                    var p=document.getElementById("indx").value;

                    
                    if(v=="" || p==""){
                        document.getElementById("notify").innerHTML="You must enter a value and its index "
                    }
                    
                    else{
                        
                        document.getElementById("notify").innerHTML= "";
                        
                        if(list.length==7){
                            document.getElementById("notify").innerHTML= "Maximum size is 8";
                        }
                        
                        else if(p>=list.length){
                            var t=list.length-1;
                            document.getElementById("notify").innerHTML= "Please enter a valid index, between 0 and "+t;
                        }
                        
                        else{
                            if(exist(v)){
                                document.getElementById("notify").innerHTML="No duplicate values allowed";
                            }
                            else{
                                document.getElementById("notify").innerHTML="";
                                for (i = list.length-1; i>=p ; i--) {
                                    list[i+1] = list[i];
                                }

                                list[p]=v;
                                display();

                                document.getElementById("action").innerHTML="You inserted "+v+" at index "+p;
                            }
                        }
                    }
                    
                    
                        document.getElementById("inI").value='';
                        document.getElementById("indx").value='';
                                               
                }

                function remove(){
                    document.getElementById("createRnd").style.display='none';
                    document.getElementById("createDef").style.display='none';
                    document.getElementById("insH").style.display='none';
                    document.getElementById("insT").style.display='none';
                    document.getElementById("insI").style.display='none';
                    document.getElementById("remH").style.display='inline-block';
                    document.getElementById("remT").style.display='inline-block';
                    document.getElementById("remI").style.display='inline-block';
                    document.getElementById("showRI").style.display='none';   
                    document.getElementById("sAr").style.display='none';
                    
                }

                function removeHead(){

                    document.getElementById("showRI").style.display='none'; 
                    document.getElementById("remH").style.display='none';
                    document.getElementById("remT").style.display='none';
                    document.getElementById("remI").style.display='none';
                    document.getElementById("notify").innerHTML= "";
                    
                    if(list.length==0){
                            document.getElementById("notify").innerHTML= "The list is empty!";
                        }
                    else{

                                document.getElementById("remHM").style.display='inline';
                                document.getElementById("method").style.display='inline';
                                document.getElementById("upR").style.left=450;
                                 var v=list.shift();
                                                                                                
                                setTimeout(function(){document.getElementById("rh1").style.background='#fbe8a6';
                                                    },1000);
                                
                                setTimeout(function(){document.getElementById("rh1").style.background='none';
                                                    document.getElementById("rh2").style.background='#fbe8a6';
                                                    },2000);
                                
                                setTimeout(function(){document.getElementById("rh3").style.background='#fbe8a6';
                                                    document.getElementById("rh2").style.background='none';
                                                    document.getElementById("upR").style.display='inline';
                                                    document.getElementById("upH").style.display='none';
                                                    
                                                    },3000);
                                
                                setTimeout(function(){document.getElementById("rh4").style.background='#fbe8a6';
                                                    document.getElementById("rh3").style.background='none';
                                                                                                                                       
                                                    },4000);

                                setTimeout(function(){document.getElementById("rh4").style.background='none';
                                                    document.getElementById("rh5").style.background='#fbe8a6';
                                                    document.getElementById("action").innerHTML="You removed the head "+ v; 
                                                   },5000);

                                setTimeout(function(){document.getElementById("upR").style.display='none';
                                                    this.display();
                                                    document.getElementById("rh5").style.background='none';
                                                    },5500);
                             
                                setTimeout(function(){document.getElementById("remHM").style.display='none';
                                                    document.getElementById("method").style.display='none';},8000);



                        document.getElementById("notify").innerHTML= "";
                       
                    }
                }

                function removeTail(){

                    document.getElementById("showRI").style.display='none';
                    document.getElementById("remH").style.display='none';
                    document.getElementById("remT").style.display='none';
                    document.getElementById("remI").style.display='none';
                    document.getElementById("notify").innerHTML= "";
                    
                    if(list.length==0){
                            document.getElementById("notify").innerHTML= "The list is empty!";
                        }
                    else{
                        
                                document.getElementById("remTM").style.display='inline';
                                document.getElementById("method").style.display='inline';
                                document.getElementById("current").style.left=397;
                                                                                                
                                setTimeout(function(){document.getElementById("rt1").style.background='#fbe8a6';
                                                    },1000);
                                
                                setTimeout(function(){document.getElementById("rt1").style.background='none';
                                                    document.getElementById("rt2").style.background='#fbe8a6';
                                                    },2000);

                                if(list.length==1){
                                     setTimeout(function(){document.getElementById("rt2").style.background='none';
                                                    document.getElementById("rt3").style.background='#fbe8a6';
                                                    },3000);

                                    setTimeout(function(){document.getElementById("rt3").style.background='none';
                                                    document.getElementById("rt4").style.background='#fbe8a6';
                                                    },4000);
                                     
                                    setTimeout(function(){document.getElementById("rt4").style.background='none';
                                                    document.getElementById("rt5").style.background='#fbe8a6';
                                                    var v=list.pop();
                                                    document.getElementById("action").innerHTML="You removed the tail "+ v;
                                                    display();
                                                    },5000);

                                    setTimeout(function(){document.getElementById("rt5").style.background='none';
                                                    document.getElementById("rt6").style.background='#fbe8a6';
                                                    },6000);

                                    setTimeout(function(){document.getElementById("rt6").style.background='none';
                                                    },6500);

                                    setTimeout(function(){document.getElementById("remTM").style.display='none';
                                                    document.getElementById("method").style.display='none';},8000);                
                                                    
                                }

                                else{

                                    setTimeout(function(){document.getElementById("rt7").style.background='#fbe8a6';
                                                        document.getElementById("rt2").style.background='none';
                                                        document.getElementById("l1").style.background='#fbe8a6';
                                                        document.getElementById("current").style.display='inline';                                                                                 
                            
                                                        },3000);
                                    
                                    setTimeout(function(){document.getElementById("rt8").style.background='#fbe8a6';
                                                        document.getElementById("rt7").style.background='none';
                                                        },4000);

                                    var c=1;
                                    if(list.length>=3){
                                        
                                        while(c<list.length-1){
