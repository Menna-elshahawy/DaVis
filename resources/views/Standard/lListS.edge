                                                    document.getElementById("ih2").style.background='none';
                                                    if(list.length>1){
                                                        document.getElementById("rightH").style.display='inline';
                                                    }
                                                    },3000);
                                
                                setTimeout(function(){document.getElementById("ih4").style.background='#fbe8a6';
                                                    document.getElementById("ih3").style.background='none';
                                                    document.getElementById("upH").style.display='none';
                                                    document.getElementById("upL").style.display='inline';
                                                                                    
                                                    },4000);

                                setTimeout(function(){document.getElementById("ih4").style.background='none';
                                                    document.getElementById("ih5").style.background='#fbe8a6';
                                                    },5000);

                                setTimeout(function(){nodeH.style.display='none';
