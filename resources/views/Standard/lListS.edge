                                                            },2000*c+3000);
                                        
                                            setTimeout(function(){document.getElementById("rt8").style.background='#fbe8a6';
                                                            document.getElementById("rt9").style.background='none';                           
                                                            },2000*c+4000);
                                            })(c);
                                            

                                            c++;
                                        }  
                                    }                

                                    setTimeout(function(){document.getElementById("rt8").style.background='none';
