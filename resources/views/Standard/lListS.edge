                    document.getElementById("insT").style.display='none';
                    document.getElementById("insI").style.display='none';
                    document.getElementById("showH").style.display='none';
                    
                    var v = document.getElementById("inH").value;
                    
                    if(v==""){
                        document.getElementById("notify").innerHTML="You must enter a value";
                    }
                    
                    else if(list.length==7){
                            document.getElementById("notify").innerHTML="Maximum length is 7";                        
                            document.getElementById("notify").innerHTML="";
                        
                        
                    }
                    else if(exist(v)){
                                document.getElementById("notify").innerHTML="No duplicate values allowed";
                            }
                    else{


                                document.getElementById("insHM").style.display='inline';
                                document.getElementById("method").style.display='inline';
                                document.getElementById("upL").style.left=376;

                                list.unshift(v);            
                                                             
                                const nodeH=document.getElementById("nodeH");
                                
                                
                                document.getElementById("nodeH").innerHTML=v;
                                
                                setTimeout(function(){document.getElementById("ih1").style.background='#fbe8a6';
                                                    },1000);
                                
                                setTimeout(function(){document.getElementById("ih1").style.background='none';
                                                    document.getElementById("ih2").style.background='#fbe8a6';
                                                    nodeH.style.display='inline-block';
                                                    },2000);
                                
