                <script>
                     
                    
                    var stack= [];
                    var length=0;
                    var f =true;
                    var cont= document.createElement('div');
                    var ul = document.createElement('ul');
                    ul.className="ul";
                    
                    function push(){
                        
                        document.getElementById("pushAr").style.display='inline-block';
                        document.getElementById("createRnd").style.display='none';
                        document.getElementById("rem").style.display='none';
                        document.getElementById("action").innerHTML="";
                        document.getElementById("srch").style.display='none';
                    }
                    
                    function go(){
                        
                        document.getElementById("pushAr").style.display='none';
                        
                        var v =document.getElementById("val").value;
                        
                        if(v ==""){
                            document.getElementById("notify").innerHTML= "You must enter a value";
                        }
                        else if(length==0){
                            document.getElementById("notify").innerHTML= "You must create a hash table first";
                            document.getElementById("val").value="";

                        }
                        else{
                            document.getElementById("notify").innerHTML= "";
                            document.getElementById("val").value="";

                            var r=v%length;

                            var n1='l'.concat(r+1).concat(1);
                            var n2='l'.concat(r+1).concat(2);
                            var n3='l'.concat(r+1).concat(3);

                            var a1='d'.concat(r+1);
                            var a2=a1.concat(r+1);
                            var a3=a2.concat(r+1);

                            if(document.getElementById(n1).style.display=='none'){
                                document.getElementById(n1).style.display='flex';
                                document.getElementById(n1).innerHTML=v;
                                document.getElementById(a1).style.display='inline';
                                 document.getElementById("action").innerHTML="You inserted "+v;
                            }
                            else if(document.getElementById(n2).style.display=='none'){
                                if(document.getElementById(n1).innerHTML==v){
                                    document.getElementById("notify").innerHTML= "The value "+ v +" is already in the hash table";
                                    document.getElementById("val").value="";
                                }
                                else{
                                    document.getElementById(n2).style.display='flex';
                                    document.getElementById(n2).innerHTML=v;
                                    document.getElementById(a2).style.display='inline';
                                    document.getElementById("action").innerHTML="You inserted "+v;
                                }
                            }
                            else if(document.getElementById(n3).style.display=='none'){
                                if(document.getElementById(n1).innerHTML==v || document.getElementById(n2).innerHTML==v){
                                    document.getElementById("notify").innerHTML= "The value "+ v +" is already in the hash table";
                                    document.getElementById("val").value="";
                                }
                                else{
                                    document.getElementById(n3).style.display='flex';
                                    document.getElementById(n3).innerHTML=v;
                                    document.getElementById(a3).style.display='inline';
                                    document.getElementById("action").innerHTML="You inserted "+v;
                                }
                            }

                            else{
                                document.getElementById("notify").innerHTML= "Maximum height is 3";
                                document.getElementById("val").value="";

                            }
                        }

                    }

                    function remove(){
                        document.getElementById("pushAr").style.display='none';
                        document.getElementById("createRnd").style.display='none';                       
                        document.getElementById("rem").style.display='inline-block';
                        document.getElementById("srch").style.display='none';
                    }      

                    function remGo(){

                        document.getElementById("rem").style.display='none';
                        var v =document.getElementById("Rval").value;
                        
                        if(v ==""){
                            document.getElementById("notify").innerHTML= "You must enter a value";
                        }
                        else if(length==0){
                            document.getElementById("notify").innerHTML= "The hash table is empty";
                            document.getElementById("Rval").value="";

                        }
                        else{
                            document.getElementById("notify").innerHTML= "";
                            document.getElementById("Rval").value="";

                            var r=v%length;

                            var n1='l'.concat(r+1).concat(1);
                            var n2='l'.concat(r+1).concat(2);
                            var n3='l'.concat(r+1).concat(3);

                            var a1='d'.concat(r+1);
                            var a2=a1.concat(r+1);
                            var a3=a2.concat(r+1);

                            if(document.getElementById(n1).style.display!='none'){
                                if(document.getElementById(n1).innerHTML==v){

                                    if(document.getElementById(n3).style.display!='none'){                               
                                        document.getElementById(n1).innerHTML=document.getElementById(n2).innerHTML;
                                        document.getElementById(n2).innerHTML=document.getElementById(n3).innerHTML;
                                        document.getElementById(n3).style.display='none';
                                        document.getElementById(a3).style.display='none';
                                    }
                                    else if(document.getElementById(n2).style.display!='none'){
                                        document.getElementById(n1).innerHTML=document.getElementById(n2).innerHTML;
                                        document.getElementById(n2).style.display='none';
                                        document.getElementById(a2).style.display='none';
                                    }
                                    else{
                                        document.getElementById(n1).style.display='none';
                                        document.getElementById(a1).style.display='none';
                                    }

                                    document.getElementById("action").innerHTML="You removed "+v;      
                                }

                                else if(document.getElementById(n2).style.display!='none'){
                                    if(document.getElementById(n2).innerHTML==v){

                                        if(document.getElementById(n3).style.display!='none'){                               
                                            document.getElementById(n2).innerHTML=document.getElementById(n3).innerHTML;
                                            document.getElementById(n3).style.display='none';
                                            document.getElementById(a3).style.display='none';
                                        }
                                        else{
                                            document.getElementById(n2).style.display='none';
                                            document.getElementById(a2).style.display='none';
                                        }
                                        document.getElementById("action").innerHTML="You removed "+v;  
                                    }

                                    else if(document.getElementById(n3).style.display!='none'){
                                        if(document.getElementById(n3).innerHTML==v){
                                            document.getElementById(n3).style.display='none';
                                            document.getElementById(a3).style.display='none';
                                            document.getElementById("action").innerHTML="You removed "+v;  
                                        }
                                        else{
                                            document.getElementById("action").innerHTML=v +" is not in the hash table";  
                                        }

                                    }
                                    else{
                                        document.getElementById("action").innerHTML=v +" is not in the hash table";  
                                        }

                                }
                                else{
                                    document.getElementById("action").innerHTML=v +" is not in the hash table";  
                                 }
                                
                            }
                            else{
                                document.getElementById("action").innerHTML=v +" is not in the hash table";  
                                }

                        }
   
                    }
                            
                    function search(){
                        var list=document.getElementById("area");
                        document.getElementById("pushAr").style.display='none';
                        document.getElementById("createRnd").style.display='none';
                        document.getElementById("rem").style.display='none';
                        document.getElementById("srch").style.display='inline-block';
                        document.getElementById("notify").innerHTML= "";
                                            
                    }

                    function searchGo(){
                        document.getElementById("srch").style.display='none';

                        var v =document.getElementById("Sval").value;
                        
                        if(v ==""){
                            document.getElementById("notify").innerHTML= "You must enter a value";
                        }
                        else if(length==0){
                            document.getElementById("notify").innerHTML= "The hash table is empty";
                            document.getElementById("Sval").value="";

                        }
                        else{
                            document.getElementById("notify").innerHTML= "";
                            document.getElementById("Sval").value="";

                            var r=v%length;

                            var n1='l'.concat(r+1).concat(1);
                            var n2='l'.concat(r+1).concat(2);
                            var n3='l'.concat(r+1).concat(3);

                            var a1='d'.concat(r+1);
                            var a2=a1.concat(r+1);
                            var a3=a2.concat(r+1);

                            if(document.getElementById(n1).style.display!='none'){
                                if(document.getElementById(n1).innerHTML==v){
                                    document.getElementById("action").innerHTML=v+ " is found in the hash table";      
                                }

                                else if(document.getElementById(n2).style.display!='none'){
                                    if(document.getElementById(n2).innerHTML==v){
                                        document.getElementById("action").innerHTML=v+ " is found in the hash table";  
                                    }

                                    else if(document.getElementById(n3).style.display!='none'){
                                        if(document.getElementById(n3).innerHTML==v){
                                            document.getElementById("action").innerHTML=v+ " is found in the hash table";  
                                        }
                                        else{
                                            document.getElementById("action").innerHTML=v +" is not found in the hash table";  
                                        }

                                    }
                                    else{
                                        document.getElementById("action").innerHTML=v +" is not found in the hash table";  
                                        }

                                }
                                else{
                                    document.getElementById("action").innerHTML=v +" is not found in the hash table";  
                                 }
                                
                            }
                            else{
                                document.getElementById("action").innerHTML=v +" is not found in the hash table";  
                                }

                        }
                    }

                    function create(){
                        document.getElementById("createRnd").style.display='inline-block';
                        document.getElementById("pushAr").style.display='none';
                        document.getElementById("showRnd").style.display='none';
                        document.getElementById("action").innerHTML="";
                        document.getElementById("srch").style.display='none';
                        document.getElementById("rem").style.display='none';
                    }

                    function showSz(){
                        document.getElementById("showRnd").style.display='inline-block';
                    }

                    function createRnd(){
                        document.getElementById("createRnd").style.display='none';
                        document.getElementById("showRnd").style.display='none';
                        
                        var l=document.getElementById("size").value;

                        if(l==""){
                            document.getElementById("notify").innerHTML= "You must enter a value";
                            document.getElementById("size").value="";
                        }
                        else if(l>7){
                            document.getElementById("notify").innerHTML= "Maximum size is 7";
                            document.getElementById("size").value="";

                        }
                        else if(l<4){
                             document.getElementById("notify").innerHTML= "Minimum size is 4";
                            document.getElementById("size").value="";
                        }
                        else{
                            length=l;
                            document.getElementById("notify").innerHTML= "";
                            document.getElementById("size").value="";
                            
                            for(i=1;i<=l;i++){
                                var n='l'.concat(i);
                                var t='i'.concat(i);
                                document.getElementById(n).style.display='inline';
                                document.getElementById(t).style.display='inline';

                                var x=n.concat('1');
                                var y=n.concat('2');
                                var z=n.concat('3');
                                var a='d'.concat(i);
                                var b=a.concat(i);
                                var c=b.concat(i);

                                document.getElementById(x).style.display='none';
                                document.getElementById(y).style.display='none';
                                document.getElementById(z).style.display='none';
                                document.getElementById(a).style.display='none';
                                document.getElementById(b).style.display='none';
                                document.getElementById(c).style.display='none';
                            }

                            for(j=parseInt(l)+1;j<=7;j++){
                                var n='l'.concat(j);
                                var t='i'.concat(j);
                                document.getElementById(n).style.display='none';
                                document.getElementById(t).style.display='none';

                                var x=n.concat('1');
                                var y=n.concat('2');
                                 var z=n.concat('3');
                                var a='d'.concat(j);
                                var b=a.concat(j);
                                var c=b.concat(j);

                               document.getElementById(x).style.display='none';
                                document.getElementById(y).style.display='none';
                                document.getElementById(z).style.display='none';
                                document.getElementById(a).style.display='none';
                                document.getElementById(b).style.display='none';
                                document.getElementById(c).style.display='none';
                            }


                        } 
                    }


                    function display(){
                        
                        var list=document.getElementById("area");
                            list.appendChild(cont);
                            cont.innerHTML="";
                            cont.appendChild(ul);
                            ul.innerHTML="";
                            for(i=stack.length-1;i>=0;i--){
                                let li = document.createElement('li');
                                li.innerHTML = stack[i];
                                ul.appendChild(li);   
                            }

                            if(stack.length==0){
                                document.getElementById("top").style.display='none';
                                document.getElementById("left").style.display='none';
                            }
                            else{
                                document.getElementById("top").style.display='inline';
                                document.getElementById("left").style.display='inline';
                            }
                    }

                    function popUpSz(){
                        document.getElementById("popSz").style.display='inline-block';
                    }

                    function disSz(){
                        document.getElementById("popSz").style.display='none';   
                    }

                    function showBack(){
                        document.getElementById("back").style.display='inline-block';
                    }

                    function disBack(){
                        document.getElementById("back").style.display='none';
                    }

                    function showBackH(){
                        document.getElementById("backH").style.display='inline-block';
                    }

                    function disBackH(){
                        document.getElementById("backH").style.display='none';
                    }


                    function show() {
                        document.getElementById("myModal").style.display = "block";
                    }

                    function closeM() {
                        document.getElementById("myModal").style.display = "none";
                    }

                    window.onclick = function(event) {
                        if (event.target == document.getElementById("myModal")) {
                            document.getElementById("myModal").style.display = "none";
                        }
                    }

                    function view(){
                        document.getElementById("method").style.display='inline';
                        document.getElementById("view").style.display='none';
                        document.getElementById("code").style.display='block';
                        document.getElementById("hide").style.display='inline';
                    }

                    function hide(){
                        document.getElementById("code").style.display='none';
                        document.getElementById("hide").style.display='none';
                            document.getElementById("view").style.display='inline';

                    }
                </script>

