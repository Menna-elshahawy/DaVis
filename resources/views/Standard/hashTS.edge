                   <div class="method" id="method"> 
                       
                       <button id="view" class="view" onclick="view()"> View Code Tracer </button>
                       <button id="hide" class="hide" onclick="hide()"> Hide Code Tracer </button>
                       
                       <div class="code" id="code">
                           
                          <div id="push" class="push">
                               <div id="mp1" class="mp1">  public void push(int elem) { </div> 
                               <div id="mp2" class="mp2" style="text-indent: 40px;">  top++; </div> 
                               <div id="mp3" class="mp3" style="text-indent: 40px;">  theStack[top] = elem; </div> 
                               <div id="mp4" class="mp4">  } </div> 
                          </div>

                           <div class="pop" id="pop">
                                <div id="pp1" class="pp1"> public int pop() { </div>
                                <div id="pp2" class="pp2" style="text-indent: 40px;"> int result = theStack[top]; </div>
                                <div id="pp3" class="pp3" style="text-indent: 40px;"> top--;</div>
                                <div id="pp4" class="pp4" style="text-indent: 40px;"> return result; </div>
                                <div id="pp5" class="pp5"> } </div>

                           </div>

                           <div class="peek" id="peek">
                                <div id="pe1" class="pe1"> public int peek() {  </div>
                                <div id="pe2" class="pe2" style="text-indent: 40px;"> return theStack[top]; </div>
                                <div id="pe3" class="pe3"> }  </div>

                           </div>
                           
                        </div>
                       
